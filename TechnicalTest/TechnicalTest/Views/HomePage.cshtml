@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.HomePage>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
	Layout = "Master.cshtml";
}



  <body>

    <main role="main">

      <section class="jumbotron text-center">
        <div class="container">
          <h1 class="jumbotron-heading">@Umbraco.Field("bannerTitle", recursive: true)</h1>
          <p class="lead text-muted">@Umbraco.Field("bannerStrapline", recursive: true)</p>
          <p>
            <a href="/Detail" class="btn btn-primary my-2">@Umbraco.Field("buttonText")</a>
          </p>
        </div>
      </section>

      <div class="album py-5 bg-light">
        <div class="container">

          <div class="row">
            
            
                @Umbraco.RenderMacro("listDoctType")
            
            
          </div>
        </div>
      </div>
      
      

    </main>

  </body>

